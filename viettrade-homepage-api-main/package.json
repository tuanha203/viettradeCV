  "scripts": {
    "build": "tsc",
    "check": "npm run prettier && npm run eslint",
    "clean": "rimraf ./dst ./docs ./coverage \"npm-debug.log*\" ./.nyc_output",
    "start": "node dst/server.js",
    "devStart": "nodemon -L index.js",
    "test": "npm run check",
    "eslint": "eslint --ext .ts src/",
    "eslint-fix": "eslint --fix --ext .ts src/",
    "preversion": "npm run clean && npm run build && npm test && npm run doc",
    "version": "git add -A",
    "postversion": "git push origin --tags",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.ts\"",
    "migrate": "npm run clean && npm run build && sequelize db:migrate",
    "migrate-w-dummy-data": "npm run clean && npm run build-w-dummy-data && sequelize db:migrate",
    "seeder": "npm run clean && npm run build && sequelize db:seed:all --seeders-path dst/seeders",
    "undo": "sequelize db:migrate:undo",
    "undo-all": "sequelize db:migrate:undo:all"
  }
